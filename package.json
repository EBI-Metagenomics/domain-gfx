{
  "name": "domain-gfx",
  "version": "0.1.4",
  "description": "Domain graphics drawing library",
  "main": "dist/domain_gfx.es5.js",
  "jsnext": "src/index.js",
  "module": "src/index.js",
  "author": "Aur√©lien Luciani <luciani@ebi.ac.uk> (https://github.com/aurel-l)",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProteinsWebTeam/domain_gfx.git"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "lodash-es": "4.17.4"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "codemirror": "5.31.0",
    "css-loader": "0.28.7",
    "eslint": "4.11.0",
    "eslint-config-prettier": "2.7.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.6.0",
    "filesize": "3.5.11",
    "flow-bin": "0.59.0",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "jest": "21.2.1",
    "jsdom": "11.3.0",
    "json-loader": "0.5.7",
    "lint-staged": "5.0.0",
    "lodash.debounce": "4.0.8",
    "npm-run-all": "4.1.2",
    "prettier": "1.8.2",
    "uglifyjs-webpack-plugin": "1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix --ignore-path .gitignore --ignore-pattern demo",
        "prettier --single-quote --trailing-comma es5 --write",
        "git add"
      ]
    }
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    },
    "presets": [
      "stage-2"
    ]
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "testRegex": ".*.test.js",
    "transformIgnorePatterns": []
  },
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "npm run test",
    "build": "webpack --env.production",
    "build:demo": "webpack --env.production --env.demo",
    "start": "webpack-dev-server",
    "test": "npm-run-all -p --aggregate-output test:lint && npm run test:flow && npm run test:jest",
    "test:lint": "eslint --color --ignore-path .gitignore \"./**/*.js\" --ignore-pattern demo",
    "test:flow": "flow check",
    "test:jest": "jest --notify",
    "publish:demo": "npm run build:demo && git subtree push --prefix demo origin gh-pages",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "domain",
    "graphics",
    "hmmer",
    "pfam",
    "ebi",
    "embl"
  ],
  "license": "Apache-2.0"
}
